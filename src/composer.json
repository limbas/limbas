{
  "name": "limbas/limbas",
  "type": "project",
  "description": "Limbas is a database framework for creating database-driven business applications. As a graphical database front-end, it enables the efficient processing of data stocks and the flexible development of comfortable low-code database applications.",
  "keywords": ["framework", "database", "limbas"],
  "license": "GPL-2.0",
  "version": "5.4.0",
  "require": {
    "php": "^8.1",
    "clue/socket-raw": "v1.5.0",
    "mpdf/mpdf": "v8.2.2",
    "openspout/openspout": "v4.24.1",
    "sabre/dav": "4.1.5",
    "scssphp/scssphp": "^v1.12.1",
    "setasign/fpdi": "v2.3.6",
    "symfony/http-foundation": "^v7.1.3",
    "symfony/http-kernel": "^v7.1.3",
    "symfony/mailer": "^v7.1.2",
    "symfony/routing": "^v7.1.3",
    "tecnickcom/tcpdf": "^6.7.5",
    "ext-sockets": "*",
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-odbc": "*",
    "ext-zlib": "*",
    "ext-mbstring": "*",
    "ext-imap": "*",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-simplexml": "*",
    "ext-ctype": "*",
    "ext-imagick": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2.6"
  },
  "autoload": {
    "psr-4": {"Limbas\\": "limbas_src/"}
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
